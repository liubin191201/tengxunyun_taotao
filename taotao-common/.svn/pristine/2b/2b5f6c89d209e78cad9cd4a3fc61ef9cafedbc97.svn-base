package com.taotao.common.pojo;

/**
 * searchBean
 * @author fenguriqi
 * 2017年5月15日 下午2:17:28
 * SearchBean
 */
public class SearchBean {
	
		/**
		 * id
		 */
		private long id ;
		
		/**
		 * 主题
		 */
		private String title;
	   
		/**
		 * 卖点
		 */
		private String sell_point;
		
		/**价格*/
		private long price;
		
		/**图片*/
		private String images;
		
		/***/
		private String item_cat_name;
		
		/**描述*/
		private String item_desc;


		public String getTitle() {
			return title;
		}

		public void setTitle(String title) {
			this.title = title;
		}

		public String getSell_point() {
			return sell_point;
		}

		public void setSell_point(String sell_point) {
			this.sell_point = sell_point;
		}

		public long getId() {
			return id;
		}

		public void setId(long id) {
			this.id = id;
		}

		public long getPrice() {
			return price;
		}

		public void setPrice(long price) {
			this.price = price;
		}

		public String getImages() {
			if(images!=null && !images.equals("")){
				String[] string=images.split(",");
				return string[0];
			}
			return images;
		}

		public void setImages(String images) {
			this.images = images;
		}
		
		public String getItem_cat_name() {
			return item_cat_name;
		}

		public void setItem_cat_name(String item_cat_name) {
			this.item_cat_name = item_cat_name;
		}

		public String getItem_desc() {
			return item_desc;
		}

		public void setItem_desc(String item_desc) {
			this.item_desc = item_desc;
		}
}
